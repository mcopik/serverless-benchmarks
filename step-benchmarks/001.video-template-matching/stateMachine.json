{
  "Comment": "A Hello World example of the Amazon States Language using Pass states",
  "StartAt": "SplitVideo",
  "States": {
    "SplitVideo": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:us-east-1:203050030216:function:splitVideo",
      "Next": "MapFrames"
    },
    "MapFrames": {
      "Type": "Map",
      "MaxConcurrency": 0,
      "ItemsPath": "$.items",
      "Iterator": {
        "StartAt": "DetectTemplate",
        "States": {
          "DetectTemplate": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:us-east-1:203050030216:function:detectTemplate",
            "End": true
          }
        }
      },
      "Next": "VerifyResults"
    },
    "VerifyResults": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:us-east-1:203050030216:function:verifyResults",
      "End": true
    }
  }
}
